@using Basyc.MessageBus.Manager.Application.ResultDiagnostics

<logWindowView>
    @for (int logEntryIndex = 0; logEntryIndex < LoggingContext.LogEntries.Count; logEntryIndex++)
    {
        var logEntry = LoggingContext.LogEntries[logEntryIndex];
        <LogEntryView LogEntry="logEntry" AlternateColor="logEntryIndex % 2 != 0" />
    }
</logWindowView>


@code {
    [Parameter] public ResultLoggingContext LoggingContext { get; set; }
}
